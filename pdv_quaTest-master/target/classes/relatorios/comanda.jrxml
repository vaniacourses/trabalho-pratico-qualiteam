	<parameter name="codvenda" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select 
	v.codigo as codvenda,
    v.valor_produtos,
    v.valor_desconto,
    v.data_cadastro,
    p.codigo as codprod,
    p.descricao as descprod,
    p.valor_venda,
    p.balanca,
    vp.valor_balanca,
    pes.nome as cliente
from
	venda v,
    venda_produtos vp,
    produto p,
    pessoa pes
where 
	pes.codigo = v.pessoa_codigo
and
	vp.venda_codigo = v.codigo
and 
	p.codigo = vp.produto_codigo
and
	v.codigo = $P{codvenda}]]>
	</queryString>
